    "node": "22.17.0",
    "npm": "please use pnpm",
    "yarn": "please use pnpm"
  },
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "dev": "next dev -p 4001",
    "build": "next build",
    "start": "next start -p 8080",
    "start:local": "pnpm build && pnpm start",
    "cz": "git-cz",
    "lint": "biome check src",
    "lint:fix": "biome check --write --unsafe src",
    "test": "vitest",
    "test:ci": "vitest --maxWorkers=1",
    "tsc": "tsc",
    "tsc:scripts": "tsc --p ./scripts/",
    "ts-node": "ts-node",
    "codegen": "pnpm codegen:download && graphql-codegen",
    "codegen:watch": "pnpm codegen:download && graphql-codegen --watch",
    "codegen:download": "env-cmd -f ./.env.local node scripts/codegen/download.js",
    "codegen:prod": "pnpm codegen:download:prod && graphql-codegen",
    "codegen:download:prod": "echo ENV_JSON > ./env.json && env-cmd -f ./.env.json node scripts/codegen/download.js",
    "codegen:ci": "graphql-codegen",
    "new:queryMock": "ts-node ./scripts/newQueryMock.ts",
    "new:mutationMock": "ts-node ./scripts/newMutationMock.ts",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic",
    "chromatic:local": "env-cmd -f ./.env.local npx chromatic"
  },
